<?xml version="1.0" encoding="UTF-8"?>
<SNSimScenario>
    <Parameters>
        <Seed>abcd</Seed>
        <GoldWeight>10</GoldWeight>
        <JobCount>25</JobCount>
    </Parameters>
    <ResourcePools>
        <ResourcePool>
            <Identifier>ResourcePool01</Identifier>
            <Resources>
                <CPU>256</CPU>
                <Memory>256000</Memory>
                <Bandwidth>300</Bandwidth>
            </Resources>
        </ResourcePool>
    </ResourcePools>
    <Customers>
        <Customer>
            <Identifier>Customer01</Identifier>
            <isGold>False</isGold>
        </Customer>
        <Customer>
            <Identifier>Customer02</Identifier>
            <isGold>True</isGold>
        </Customer>
    </Customers>
    <Services>
        <Service>
            <Identifier>A</Identifier>
            <ResourcePool>ResourcePool01</ResourcePool>
            <Ticks>8</Ticks>
            <Resources>
                <CPU>1</CPU>
                <Memory>64</Memory>
                <Bandwidth>0</Bandwidth>
            </Resources>
            <MaxAttempts>100</MaxAttempts>
        </Service>
        <Service>
            <Identifier>B</Identifier>
            <ResourcePool>ResourcePool01</ResourcePool>
            <Ticks>4</Ticks>
            <Resources>
                <CPU>1</CPU>
                <Memory>128</Memory>
                <Bandwidth>0</Bandwidth>
            </Resources>
            <MaxAttempts>100</MaxAttempts>
        </Service>
        <Service>
            <Identifier>C</Identifier>
            <ResourcePool>ResourcePool01</ResourcePool>
            <Ticks>16</Ticks>
            <Resources>
                <CPU>2</CPU>
                <Memory>512</Memory>
                <Bandwidth>6</Bandwidth>
            </Resources>
            <MaxAttempts>100</MaxAttempts>
        </Service>
        <Service>
            <Identifier>D</Identifier>
            <ResourcePool>ResourcePool01</ResourcePool>
            <Ticks>12</Ticks>
            <Resources>
                <CPU>2</CPU>
                <Memory>128</Memory>
                <Bandwidth>12</Bandwidth>
            </Resources>
            <MaxAttempts>100</MaxAttempts>
        </Service>
    </Services>
    <JobTemplates>
        <JobTemplate>
            <Identifier>Pattern01</Identifier>
            <Signature>(('A'), ('C'), ('B', 'D'))</Signature>
            <Revenue>50</Revenue>
            <Penalty>80</Penalty>
        </JobTemplate>
        <JobTemplate>
            <Identifier>Pattern02</Identifier>
            <Signature>(('C'), ('B'))</Signature>
            <Revenue>20</Revenue>
            <Penalty>30</Penalty>
        </JobTemplate>
        <JobTemplate>
            <Identifier>Pattern03</Identifier>
            <Signature>(('B', 'C'), ('B'), ('A'))</Signature>
            <Revenue>70</Revenue>
            <Penalty>150</Penalty>
        </JobTemplate>
        <JobTemplate>
            <Identifier>Pattern04</Identifier>
            <Signature>(('D', 'A'), ('C'), ('B'), ('A'))</Signature>
            <Revenue>80</Revenue>
            <Penalty>90</Penalty>
        </JobTemplate>
        <JobTemplate>
            <Identifier>Pattern05</Identifier>
            <Signature>(('D', 'C'), ('A', 'B'))</Signature>
            <Revenue>35</Revenue>
            <Penalty>40</Penalty>
        </JobTemplate>
    </JobTemplates>
